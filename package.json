    "semantic-ui-react": "0.67.0"
  },
  "devDependencies": {
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "react-addons-test-utils": "^15.4.2"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
