    "express-jwt": "5.1.0",
    "history": "4.6.0",
    "jsonwebtoken": "7.3.0",
    "mongoose": "4.8.5",
    "mongoose-unique-validator": "1.0.4",
    "mongoose-validator": "1.2.5",
    "morgan": "1.8.1",
    "node-sass": "4.5.0",
    "normalize.css": "5.0.0",
    "npm-run-all": "4.0.2",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router-dom": "4.0.0",
    "react-scripts": "0.9.5",
    "recharts": "0.21.2",
